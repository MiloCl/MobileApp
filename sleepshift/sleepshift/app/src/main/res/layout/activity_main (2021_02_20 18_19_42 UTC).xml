<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnCurrentSchedule"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:background="@drawable/ic_alarm_add_24px"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toStartOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtCurrentScheduleMinute"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <Button
        android:id="@+id/btnNewSchedule"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:background="@drawable/ic_alarm_add_24px"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtNewScheduleMinute"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="5dp"
        android:layout_height="20dp"
        android:textColor="#000000"
        android:text=":"
        app:layout_constraintBottom_toBottomOf="@+id/txtNewScheduleMinute2"
        app:layout_constraintEnd_toStartOf="@+id/txtNewScheduleMinute2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtNewScheduleHour2"
        app:layout_constraintTop_toTopOf="@+id/txtNewScheduleMinute2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="26dp"
        android:layout_height="20dp"
        android:text="To:"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/txtNewScheduleHour2"
        app:layout_constraintEnd_toStartOf="@+id/txtNewScheduleHour2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnNewSchedule"
        app:layout_constraintTop_toTopOf="@+id/txtNewScheduleHour2" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:text=":"
        app:layout_constraintBottom_toBottomOf="@+id/txtNewScheduleMinute"
        app:layout_constraintEnd_toStartOf="@+id/txtNewScheduleMinute"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtNewScheduleHour"
        app:layout_constraintTop_toTopOf="@+id/txtNewScheduleMinute" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="From:"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/txtNewScheduleHour"
        app:layout_constraintEnd_toStartOf="@+id/txtNewScheduleHour"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtNewScheduleHour" />

    <Button
        android:id="@+id/btnNewSchedule2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_alarm_add_24px"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtNewScheduleMinute2"
        app:layout_constraintTop_toBottomOf="@+id/editText9" />

    <TextView
        android:id="@+id/txtNewScheduleMinute2"
        android:layout_width="27dp"
        android:layout_height="22dp"
        android:text="00"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/btnNewSchedule2"
        app:layout_constraintEnd_toStartOf="@+id/btnNewSchedule2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/btnNewSchedule2" />

    <TextView
        android:id="@+id/txtNewScheduleHour2"
        android:layout_width="26dp"
        android:layout_height="20dp"
        android:text="0"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/txtNewScheduleMinute"
        android:layout_width="28dp"
        android:layout_height="24dp"
        android:text="00"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/btnNewSchedule"
        app:layout_constraintEnd_toStartOf="@+id/btnNewSchedule"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="@+id/btnNewSchedule" />

    <TextView
        android:id="@+id/txtNewScheduleHour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="@+id/textView13"
        app:layout_constraintEnd_toStartOf="@+id/textView13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/textView13" />

    <TextView
        android:id="@+id/txtCurrentScheduleMinute2"
        android:layout_width="22dp"
        android:layout_height="21dp"
        android:text="00"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/btnCurrentSchedule2"
        app:layout_constraintEnd_toStartOf="@+id/btnCurrentSchedule2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/btnCurrentSchedule2" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To:"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/txtCurrentScheduleHour2"
        app:layout_constraintEnd_toStartOf="@+id/txtCurrentScheduleHour2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnCurrentSchedule"
        app:layout_constraintTop_toTopOf="@+id/txtCurrentScheduleHour2" />

    <TextView
        android:id="@+id/txtCurrentScheduleHour2"
        android:layout_width="23dp"
        android:layout_height="21dp"
        android:text="0"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintEnd_toStartOf="@+id/textView12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <Button
        android:id="@+id/btnCalculate"
        android:layout_width="160dp"
        android:layout_height="72dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/seek"
        android:elevation="5sp"
        android:onClick="onBtnCalculate_Clicked"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/editText6"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="50dp"
        android:background="#B73C3333"
        android:backgroundTint="#00FFFFFF"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="CURRENT SLEEP SCHEDULE "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editText9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:background="#B73C3333"
        android:backgroundTint="#00FFFFFF"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="CHOOSE NEW SCHEDULE"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCurrentScheduleMinute" />

    <Spinner
        android:id="@+id/spCurrentCountry"
        android:layout_width="225dp"
        android:layout_height="31dp"
        android:layout_marginTop="48dp"
        android:background="#F5EEEE"
        android:clickable="true"
        android:entries="@array/Land"
        android:text="Continent"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/editText12" />

    <TextView
        android:id="@+id/editText12"
        android:layout_width="269dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="70dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="JETLAG PREVENTION MODE"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnNewSchedule" />

    <Spinner
        android:id="@+id/spDest"
        android:layout_width="225dp"
        android:layout_height="31dp"
        android:layout_marginTop="15dp"
        android:background="#F0EDED"
        android:entries="@array/Land"
        android:spinnerMode="dropdown"
        android:text="City"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/spCurrentCountry"
        app:layout_constraintStart_toStartOf="@+id/spCurrentCountry"
        app:layout_constraintTop_toBottomOf="@+id/spCurrentCountry" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="From:"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/txtCurrentScheduleHour"
        app:layout_constraintEnd_toStartOf="@+id/txtCurrentScheduleHour"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtCurrentScheduleHour" />

    <TextView
        android:id="@+id/txtCurrentScheduleMinute"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="00"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/btnCurrentSchedule"
        app:layout_constraintEnd_toStartOf="@+id/btnCurrentSchedule"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="@+id/btnCurrentSchedule" />

    <Button
        android:id="@+id/btnCurrentSchedule2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_alarm_add_24px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtCurrentScheduleMinute2"
        app:layout_constraintTop_toBottomOf="@+id/editText6" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=":"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/txtCurrentScheduleMinute"
        app:layout_constraintEnd_toStartOf="@+id/txtCurrentScheduleMinute"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtCurrentScheduleHour"
        app:layout_constraintTop_toTopOf="@+id/txtCurrentScheduleMinute" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="5dp"
        android:layout_height="20dp"
        android:text=":"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/txtCurrentScheduleMinute2"
        app:layout_constraintEnd_toStartOf="@+id/txtCurrentScheduleMinute2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtCurrentScheduleHour2"
        app:layout_constraintTop_toTopOf="@+id/txtCurrentScheduleMinute2" />

    <TextView
        android:id="@+id/txtCurrentScheduleHour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView11" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/editText12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editText12"
        app:layout_constraintTop_toTopOf="@+id/editText12" />

    <ImageView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Current Country"
        app:layout_constraintEnd_toEndOf="@+id/editText12"
        app:layout_constraintStart_toStartOf="@+id/editText12"
        app:layout_constraintTop_toBottomOf="@+id/editText12"
        app:srcCompat="@drawable/ic_flight_takeoff_24px" />

    <ImageView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Destination:"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/ic_flight_land_24px" />

    <TextView
        android:id="@+id/tijdVerschil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="24dp"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spDest" />

    <CheckBox
        android:id="@+id/cbPermission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Use phone location"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/editText12"
        app:layout_constraintTop_toBottomOf="@+id/editText12" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="18dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/cbPermission"
        app:layout_constraintStart_toEndOf="@+id/cbPermission"
        app:layout_constraintTop_toBottomOf="@+id/editText12"
        app:srcCompat="@drawable/ic_my_location_24px" />

    <TextView
        android:id="@+id/white"
        android:layout_width="394dp"
        android:layout_height="88dp"
        android:background="#FFFFFF"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editText9" />


</androidx.constraintlayout.widget.ConstraintLayout>